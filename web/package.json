{
	"name": "template-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^25.0.3",
		"eip-1193": "^0.6.5",
		"ipfs-gateway-emulator": "4.2.1-ipfs.2",
		"named-logs": "^0.4.1",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"pwag": "^0.4.0",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.4",
		"tevm": "1.0.0-next.149",
		"typescript": "^5.9.3",
		"viem": "^2.43.2",
		"vite": "^7.3.0",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"scripts": {
		"prepare": "pnpm generate-pwa-icons-and-tags && svelte-kit sync || echo ''",
		"generate-pwa-icons-and-tags": "pwag static/icon.png src/web-config.json",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"serve": "ipfs-emulator --only -d build -p 8080"
	}
}
